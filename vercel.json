{
    "builds": [{
        "src": "blogapp/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.10" 
        }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "static"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "blogapp/wsgi.py"
        }
    ]
}